{
    "editor.minimap.enabled": false,
    "files.insertFinalNewline": true,
    "window.zoomLevel": 0,
    // "workbench.colorTheme": "Braver's Solarized Dark (themed workbench)",
    // "workbench.colorTheme": "Solarized Dark",

    // terminal.integrated.fontFamily
